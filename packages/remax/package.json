{
  "name": "remax",
  "version": "1.11.0-alpha.0",
  "description": "Remax 是一个全新的基于 React 的小程序开发框架",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "keywords": [
    "react",
    "miniapp",
    "mini-program",
    "wechat"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remaxjs/remax/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remaxjs/remax.git"
  },
  "homepage": "https://remaxjs.org",
  "scripts": {
    "clean": "rm -rf esm cjs",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:esm": "npm run build:esm -- -w",
    "watch:cjs": "npm run build:cjs -- -w",
    "prebuild": "npm run clean",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir cjs",
    "test": "jest",
    "test:debug": "npx --node-arg=--inspect-brk jest -i"
  },
  "dependencies": {
    "@remax/mini-types": "^0.1.0",
    "@remax/miniprogram-api-typings": "^2.8.4",
    "lodash.kebabcase": "^4.1.1",
    "react-reconciler": "0.24.0",
    "remax.macro": "^1.10.7",
    "scheduler": "0.18.0",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/node": "^12.12.11",
    "@types/react": "^16.8.22",
    "@types/react-reconciler": "^0.18.0",
    "@types/react-test-renderer": "^16.9.0",
    "@types/shallowequal": "^1.1.1",
    "concurrently": "^5.0.0",
    "jest": "^24.8.0",
    "object-path-immutable": "^4.0.2",
    "react": "^16.8.6",
    "react-test-renderer": "^16.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.7.3"
  },
  "gitHead": "28ef4f50ef3c134c4e661d786ebea4d7c4ea89ed"
}
